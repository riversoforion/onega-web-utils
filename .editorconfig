root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,jsx,ts,tsx}]
indent_style = space
indent_size = 2
max_line_length = 120
ij_javascript_use_double_quotes = false
ij_javascript_force_quote_style = true
ij_javascript_enforce_trailing_comma = whenmultiline
